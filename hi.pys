class Animal(name)
{
    var Name = name
    
    fn Speak()
    {
        print("a")
    }
}

class Dog(name, adorable) : Animal
{
    var Name = name
    var Adored = adorable
    
    fn Speak()
    {
        print("WOOF")
    }
}


var ashton_dunderdale = Dog("Ashton", true)
ashton_dunderdale.Speak()
var adored = ashton_dunderdale.Adored

print(adored)

var teams = Animal("teams")
var meWhenAshton = teams.Name
teams.Speak()
print(meWhenAshton)